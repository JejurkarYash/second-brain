{
  "version": 2,
  "builds": [
    {
      "src": "/src/(.*)",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10,
        "includeFiles": ["dist/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "regions": ["iad1"],
  "build": {
    "env": {
      "CI": "false"
    }
  }
}
